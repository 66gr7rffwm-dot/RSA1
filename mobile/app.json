{
  "expo": {
    "name": "Carpool",
    "slug": "carpooling-app",
    "version": "1.0.2",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#FFFFFF",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carpooling.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#6366F1"
      },
      "package": "com.carpooling.app",
      "versionCode": 3,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "FOREGROUND_SERVICE",
        "RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Carpool to use your location to find nearby rides and share your location during trips."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Carpool to access your photos for profile and document uploads."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3e4147ae-ed2d-4f42-b529-e9f54cb13151"
      }
    },
    "owner": "amjad4093"
  }
}
